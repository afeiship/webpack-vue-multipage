{
  "name": "webpack-vue-multipage",
  "description": "A Vue.js project",
  "author": "afeiship <1290657123@qq.com>",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "predev": "npm run clean && webpack --mode development --progress --config build/webpack.dll.conf.js",
    "prebuild": "npm run clean && webpack --mode production --progress --config build/webpack.dll.conf.js",
    "dev": "webpack-dev-server --inline --hot --config build/webpack.dev.conf.js --env=dev",
    "build": "webpack --mode production --progress --config build/webpack.prod.conf.js --env=prd"
  },
